{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "arandilopez/laravel-profane": "^0.5.0",
        "askedio/laravel5-profanity-filter": "^1.10",
        "based/momentum-trail": "^0.3.0",
        "cebe/markdown": "^1.2",
        "cjmellor/level-up": "^1.2.3",
        "cocur/slugify": "^4.3",
        "darkaonline/l5-swagger": "^8.5",
        "dg/rss-php": "^1.5",
        "doctrine/dbal": "^3.6",
        "ezyang/htmlpurifier": "^4.16",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "illuminate/pagination": "*",
        "inertiajs/inertia-laravel": "^1.0",
        "jbbcode/jbbcode": "^1.4",
        "kornrunner/keccak": "^1.1",
        "laravel-lang/lang": "^12.22",
        "laravel/framework": "^11.0",
        "laravel/octane": "^2.3",
        "laravel/passport": "^12.0",
        "laravel/reverb": "@beta",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.9",
        "laravel/socialite": "^5.14",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/html-to-markdown": "^5.1",
        "mariuzzo/laravel-js-localization": "^1.10",
        "martinbean/socialite-discord-provider": "^1.2",
        "meilisearch/meilisearch-php": "^1.8",
        "novu/novu-laravel": "^1.3",
        "php-open-source-saver/jwt-auth": "^2.1",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "simplito/elliptic-php": "^1.0",
        "spatie/laravel-responsecache": "^7.4",
        "symfony/http-client": "^6.3",
        "symfony/mailgun-mailer": "^6.3",
        "symfony/process": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.23",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },

        "files": [
            "app/VueHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": ["laravel/telescope"]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
